<tr>
  <td><%= blog.title %></td>
  <td><%= blog.body %></td>
  <td><%= link_to blog.status, change_status_blog_path(blog) %></td>
  <td><%= link_to 'Show', blog %></td>
  <td><%= link_to 'Edit', edit_blog_path(blog) %></td>
  <td><%= link_to 'Delete entry', blog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>
